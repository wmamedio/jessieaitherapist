{
  "name": "my-svelte-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --open",
    "build": "vite build",
    "preview": "npm run build && wrangler pages dev",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "deploy": "npm run build && wrangler pages deploy",
    "postbuild": "npx svelte-sitemap --out-dir .svelte-kit/cloudflare --domain https://jessietherapist.com --ignore '/404*'"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.5.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-hero-icons": "^5.1.0",
    "svelte-sitemap": "^2.6.0",
    "tailwindcss": "^3.4.3",
    "vite": "^5.0.3",
    "wrangler": "^3.57.1"
  },
  "type": "module",
  "dependencies": {
    "@sendgrid/mail": "^8.1.3",
    "@tailwindcss/typography": "^0.5.13",
    "xml2js": "^0.6.2"
  }
}
